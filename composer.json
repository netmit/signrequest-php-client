{
  "name": "signrequest/signrequest-client",
  "version": "1.0.0",
  "description": "Official PHP client for SignRequest.com (https://signrequest.com)",
  "keywords": [
    "signrequest",
    "php",
    "sdk",
    "api"
  ],
  "homepage": "https://signrequest.com/",
  "license": "MIT",
  "authors": [
    {
      "name": "SignRequest",
      "homepage": "https://signrequest.com/"
    }
  ],
  "require": {
    "php": ">=5.5",
    "ext-curl": "*",
    "ext-json": "*",
    "ext-mbstring": "*",
    "guzzlehttp/guzzle": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^4.8",
    "squizlabs/php_codesniffer": "~2.6",
    "friendsofphp/php-cs-fixer": "~1.12"
  },
  "autoload": {
    "psr-4": {
      "SignRequest\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SignRequest\\": "test/"
    }
  }
}
